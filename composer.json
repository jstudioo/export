{
    "name": "jstudioo/spreadsheet",
    "description": "Spreadsheet for yii2",
    "keywords": ["yii2", "spreadsheet"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/jstudioo/spreadsheet/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "https://github.com/jstudioo/spreadsheet/wiki",
        "source": "https://github.com/jstudioo/spreadsheet"
    },
    "authors": [
        {
            "name": "Jstudioo",
            "email": "jstudioo@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.13"
        },
    "require-dev": {
        "phpunit/phpunit": "^5.0 || ^6.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": { "jstudioo\\spreadsheet\\": "src" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
